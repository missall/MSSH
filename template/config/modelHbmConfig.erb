<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="cn.<%=@@projectName%>.model.<%=@@modelName%>" table="<%=@@bean.table_name%>">
    <id name="<%=@@bean.columns[0].last%>" type="<%=@@bean.columns[0][1]%>">
      <column name="<%=@@bean.columns[0].first%>" precision="10" scale="0" />
      <generator class="increment"></generator>
    </id><% @@bean.columns[1..@@bean.columns.length].each do |p|%>
    <property name="<%=p.last%>" type="<%=p[1]%>">
      <column name="<%=p.first%>"/>
    </property><%end%> 
  </class>
</hibernate-mapping>