    <to>%{context-path}/SysRoleAction.action?method=list</to>
  </rule>
	
  <rule>
    <from>/sysRole/edit/([0-9]+)$</from>
    <to>%{context-path}/PreUpdateSysRoleAction.action?method=preUpdate&amp;sysRoleId=$1</to>
